# Backend example environment variables
# Copy to backend/.env and fill values before running

# MongoDB (Atlas or local). Example local: mongodb://localhost:27017/city_management
MONGO_URI=
MONGODB_URI=

# Server
PORT=5000
NODE_ENV=development

# JWT
JWT_SECRET=change_this_to_a_strong_secret
JWT_EXPIRE=30d

# Cloudinary (optional, required for image uploads)
CLOUDINARY_CLOUD_NAME=
CLOUDINARY_API_KEY=
CLOUDINARY_API_SECRET=
